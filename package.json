{
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "cz": "git-cz",
    "test": "pnpm test:jest && pnpm test:vitest",
    "test:jest": "jest",
    "test:vitest": "vitest",
    "test:coverage": "vitest --coverage",
    "dev": "pnpm -C play dev",
    "gen": "tsx scripts/gen.ts",
    "gen:version": "tsx scripts/gen-version.ts",
    "update:version": "tsx scripts/update-version.ts",
    "build": "gulp --require @esbuild-kit/cjs-loader -f gulpfile",
    "build:theme": "pnpm run build -C packages/theme-chalk",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "prepare": "husky install",
    "postinstall": "pnpm gen:version",
    "preinstall": "only-allow pnpm -y",
    "release": "cd dist/element-ultra && npm publish --registry https://registry.npmjs.org --access public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "vue": "^3.2.37"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.6",
    "@element-ultra/components": "workspace:*",
    "@element-ultra/constants": "workspace:*",
    "@element-ultra/directives": "workspace:*",
    "@element-ultra/hooks": "workspace:*",
    "@element-ultra/test-utils": "workspace:*",
    "@element-ultra/theme-chalk": "workspace:*",
    "@element-ultra/tokens": "workspace:*",
    "@element-ultra/utils": "workspace:*",
    "@popperjs/core": "^2.11.5",
    "@vueuse/core": "^8.7.5",
    "@wangeditor/editor": "5.1.1",
    "dayjs": "^1.11.3",
    "fe-dk": "^1.0.12",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.1",
    "memoize-one": "^6.0.0",
    "sortablejs": "^1.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@esbuild-kit/cjs-loader": "^2.2.1",
    "@pnpm/find-workspace-packages": "^3.0.40",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/lodash": "^4.14.178",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.0",
    "@types/sass": "^1.43.1",
    "@types/sortablejs": "^1.13.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/tsconfig": "^0.1.3",
    "chalk": "^4.1.2",
    "components-helper": "^2.0.0",
    "csstype": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "del": "^6.1.1",
    "esbuild": "^0.14.47",
    "expect-type": "^0.13.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "only-allow": "^1.1.1",
    "pretty-quick": "^3.1.3",
    "rollup": "^2.75.7",
    "rollup-plugin-esbuild": "^4.9.1",
    "sass": "^1.53.0",
    "ts-morph": "^15.1.0",
    "tsx": "^3.6.0",
    "type-fest": "^2.15.1",
    "typescript": "^4.7.4",
    "unplugin-vue-components": "^0.20.1",
    "unplugin-vue-define-options": "^0.6.1",
    "vite": "^2.9.12",
    "vite-plugin-inspect": "^0.5.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-tsc": "^0.38.2"
  }
}
