{
  "private": true,
  "packageManager": "pnpm@6.31.0",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "cz": "git-cz",
    "test": "pnpm test:jest && pnpm test:vitest",
    "test:jest": "jest",
    "test:vitest": "vitest",
    "test:coverage": "vitest --coverage",
    "dev": "pnpm -C play dev",
    "gen": "sucrase-node scripts/gen.ts",
    "gen:version": "sucrase-node scripts/gen-version.ts",
    "update:version": "sucrase-node scripts/update-version.ts",
    "build": "gulp --require sucrase/register/ts -f gulpfile",
    "build:theme": "pnpm run build -C packages/theme-chalk",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "prepare": "husky install",
    "postinstall": "pnpm gen:version",
    "preinstall": "only-allow pnpm -y",
    "release": "cd dist/element-ultra && npm publish --registry https://registry.npmjs.org --access public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "vue": "^3.2.36"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@element-ultra/components": "workspace:*",
    "@element-ultra/constants": "workspace:*",
    "@element-ultra/directives": "workspace:*",
    "@element-ultra/hooks": "workspace:*",
    "@element-ultra/test-utils": "workspace:*",
    "@element-ultra/theme-chalk": "workspace:*",
    "@element-ultra/tokens": "workspace:*",
    "@element-ultra/utils": "workspace:*",
    "@popperjs/core": "^2.11.5",
    "@vueuse/core": "^8.1.2",
    "@wangeditor/editor": "5.0.1",
    "dayjs": "^1.10.7",
    "fe-dk": "^1.0.12",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.1",
    "memoize-one": "^6.0.0",
    "normalize-wheel-es": "^1.1.1",
    "sortablejs": "^1.15.0"
  },

  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@pnpm/find-workspace-packages": "^3.1.40",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^7.9.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.178",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.18",
    "@types/rollup-plugin-css-only": "^3.1.0",
    "@types/sass": "^1.43.1",
    "@types/sortablejs": "^1.13.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/ui": "^0.12.3",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/test-utils": "2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "babel-jest": "^26.6.3",
    "chalk": "^4.1.2",
    "components-helper": "^2.0.0",
    "csstype": "^2.6.19",
    "cz-conventional-changelog": "^3.3.0",
    "del": "^6.0.0",
    "esbuild": "^0.14.39",
    "expect-type": "^0.13.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "pretty-quick": "^3.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.74.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-filesize": "^9.1.2",
    "sass": "^1.51.0",
    "sucrase": "^3.21.0",
    "ts-jest": "^26.5.6",
    "ts-morph": "^15.0.0",
    "type-fest": "^2.12.2",
    "typescript": "^4.7.2",
    "unplugin-vue-define-options": "^0.6.1",
    "vitest": "^0.12.3",
    "vue": "^3.2.36",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-router": "^4.0.15",
    "vue-tsc": "^0.35.2",
    "only-allow": "^1.1.0"
  }
}
